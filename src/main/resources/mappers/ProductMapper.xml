        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gold.mappers.ProductMapper">
    <!-- 물품 추가 -->
    <select id="addProduct" parameterType="com.gold.model.ProductVo">
        INSERT INTO PRODUCT (PRODUCT_NAME, BRAND_ID, PRODUCT_CODE)
        VALUES(#{productName},#{brandID},#{productCode});
    </select>

</mapper>